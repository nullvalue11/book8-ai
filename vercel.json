{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/cron/sync?task=sync",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/sync?task=reminders",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/billing/usage/run-daily",
      "schedule": "0 2 * * *"
    }
  ]
}
